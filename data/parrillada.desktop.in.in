[Desktop Entry]
_Name=Parrillada
_GenericName=Disc Burner and Copier
_Comment=Create and copy CDs and DVDs
_Keywords=disc;cdrom;dvd;burn;audio;video;
Categories=GTK;X-MATE;AudioVideo;Audio;Video;DiscBurning;
MimeType=application/x-cd-image;application/x-cdrdao-toc;application/x-cue;application/x-toc;audio/x-scpls;audio/x-ms-asx;audio/x-mp3-playlist;audio/x-mpegurl;application/x-parrillada;x-content/audio-cdda;x-content/video-dvd;x-content/video-vcd;x-content/video-svcd;x-content/image-picturecd;
Exec=parrillada %U
Icon=parrillada
StartupNotify=true
Terminal=false
Type=Application
_X-MATE-FullName=Parrillada Disc Burner
X-MATE-UsesNotifications=true
Actions=Window;Image;Disc;Audio;Video;

[Desktop Action Window]
_Name=Open a New Window
Exec=parrillada --no-existing-session

[Desktop Action Image]
_Name=Burn an Image File
Exec=parrillada --image

[Desktop Action Disc]
Name=Copy a Disc
Exec=parrillada --copy

[Desktop Action Audio]
_Name=Create an Audio Project
Exec=parrillada --audio

[Desktop Action Video]
_Name=Create a Video Project
Exec=parrillada --video
